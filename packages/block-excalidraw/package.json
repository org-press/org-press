{
  "name": "@org-press/block-excalidraw",
  "version": "0.9.5",
  "description": "Excalidraw diagram block plugin for org-press",
  "license": "GPL-2.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./wrapper": {
      "import": "./dist/wrapper.js"
    }
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "index.org",
    "README.md"
  ],
  "scripts": {
    "build": "orgp build index.org --block plugin,wrapper --out dist/ && node -e \"const fs=require('fs'); fs.writeFileSync('dist/index.js', 'export { default as plugin } from \\\"./plugin.js\\\";\\n'); fs.writeFileSync('dist/index.d.ts', 'export { default as plugin } from \\\"./plugin.js\\\";\\n');\"",
    "test": "vitest",
    "test:ci": "vitest --run",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "org-press": "^0.9.0",
    "@excalidraw/excalidraw": "^0.18.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "org-press": "workspace:*",
    "typescript": "~5.7.3",
    "vitest": "^3.1.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://orgp.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/org-press/org-press.git"
  },
  "bugs": {
    "url": "https://github.com/org-press/org-press/issues"
  }
}
